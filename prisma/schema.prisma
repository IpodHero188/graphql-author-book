generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Author {
  id   Int    @id @default(autoincrement())
  name String
  book Book[]
}

model Book {
  id     Int    @id @default(autoincrement())
  title  String
  genre  String
  author Author @relation(fields: [id], references: [id])
}
